{
  "name": "playlistyv3",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "local": "concurrently -n nextjs,proxy,sqlite,drizzle-studio -c cyan,yellow,green,blue \"pnpm run dev\" \"pnpm run lt\" \"pnpm run db:dev\" \"pnpm run db:studio\"",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lt": "lt --port 3000 --subdomain playlisty",
    "db:dev": "docker run -p 8080:8080 --name playlisty-db ghcr.io/tursodatabase/libsql-server:latest",
    "db:studio": "drizzle-kit studio --port 5000"
  },
  "dependencies": {
    "@clerk/nextjs": "^4.29.2",
    "@libsql/client": "0.4.0-pre.7",
    "@radix-ui/react-slot": "^1.0.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "drizzle-orm": "^0.29.3",
    "drizzle-zod": "^0.5.1",
    "googleapis": "^130.0.0",
    "localtunnel": "^2.0.2",
    "lucide-react": "^0.307.0",
    "next": "14.0.4",
    "react": "^18",
    "react-dom": "^18",
    "svix": "^1.15.0",
    "tailwind-merge": "^2.2.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "concurrently": "^8.2.2",
    "drizzle-kit": "^0.20.10",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
