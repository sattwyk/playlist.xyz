{
  "name": "playlistyv3",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "local": "concurrently -n nextjs,proxy,libsql,drizzle-studio -c cyan,yellow,green,blue \"pnpm run dev\" \"pnpm run lt\" \"pnpm run db:dev\" \"pnpm run db:studio\"",
    "dev": "next dev",
    "preview": "next build && next start",
    "build": "next build",
    "build:debug": "next build --debug",
    "start": "next start",
    "lint": "next lint",
    "lt": "lt --port 3000 --subdomain playlisty-local-dev-tunnel",
    "db:dev": "docker compose up",
    "db:studio": "drizzle-kit studio --port 5000 --verbose --config=drizzle.config.dev.ts",
    "db:migrate": "drizzle-kit generate:sqlite --config=drizzle.config.dev.ts",
    "db:push": "drizzle-kit push:sqlite --config=drizzle.config.dev.ts",
    "db:migrate-prod": "drizzle-kit generate:sqlite --config=drizzle.config.prod.ts",
    "db:push-prod": "drizzle-kit push:sqlite --config=drizzle.config.prod.ts"
  },
